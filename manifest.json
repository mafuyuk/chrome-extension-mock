{
  "name": "Chrome Extension mock",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Chrome Extension mock.",
  "background": {
    "scripts": [
      "js/eventPage.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "js/content-script.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "js/embeded-script.js"
  ],
  "permissions": [
    "<all_urls>"
  ]
}
